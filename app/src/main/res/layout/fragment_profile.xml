<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:foregroundTint="#9E4242"
    tools:context=".ProfileFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".myactivitiesclasses.profilePageForOtherUsers">

        <LinearLayout

            android:id="@+id/linearLayout4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/jockey_one"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView

                android:id="@+id/textView4"

                android:layout_width="25dp"

                android:layout_height="80dp"

                android:layout_weight="1"

                android:background="#B3DEFF" />

            <TextView

                android:id="@+id/textView5"

                android:layout_width="25dp"

                android:layout_height="68dp"

                android:layout_weight="1"

                android:background="#8BCFFF" />


            <TextView

                android:id="@+id/textView6"

                android:layout_width="25dp"

                android:layout_height="55dp"

                android:layout_weight="1"

                android:background="#62B0FF" />


            <TextView

                android:id="@+id/textView10"
                android:layout_width="25dp"
                android:layout_height="43dp"
                android:layout_weight="1"
                android:background="#46B0FF" />


            <TextView

                android:id="@+id/textView7"

                android:layout_width="25dp"

                android:layout_height="55dp"

                android:layout_weight="1"

                android:background="#33BEFF" />


            <TextView

                android:id="@+id/textView8"

                android:layout_width="25dp"

                android:layout_height="62dp"

                android:layout_weight="1"

                android:background="#438AF8" />


            <TextView

                android:id="@+id/textView14"

                android:layout_width="31dp"

                android:layout_height="37dp"

                android:layout_weight="1"

                android:background="#6486FF" />


            <TextView

                android:id="@+id/textView9"

                android:layout_width="25dp"

                android:layout_height="25dp"

                android:layout_weight="1"

                android:background="#4094CD" />


            <TextView

                android:id="@+id/textView11"

                android:layout_width="25dp"

                android:layout_height="55dp"

                android:layout_weight="1"

                android:background="#4979B0" />


            <TextView

                android:id="@+id/textView12"

                android:layout_width="25dp"

                android:layout_height="65dp"

                android:layout_weight="1"

                android:background="#6993BD" />


            <TextView

                android:id="@+id/textView13"

                android:layout_width="25dp"

                android:layout_height="72dp"

                android:layout_weight="1"

                android:background="#739DE8" />


            <TextView

                android:id="@+id/textView15"

                android:layout_width="25dp"

                android:layout_height="34dp"

                android:layout_weight="1"

                android:background="#275583" />

        </LinearLayout>

        <LinearLayout

            android:id="@+id/linearLayout6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal">


                    <ImageView
                        android:id="@+id/imageView11"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="90dp"
                        android:layout_marginTop="60dp"
                        tools:layout_editor_absoluteX="131dp"
                        tools:layout_editor_absoluteY="42dp"
                        tools:srcCompat="@drawable/profile_photo" />

                    <ImageView
                        android:id="@+id/imageView14"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="95dp"
                        android:src="@drawable/img" />

                </LinearLayout>

                <TextView
                    android:id="@+id/nameInProfile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="15dp"
                    android:fontFamily="@font/jockey_one"
                    android:text="name surname"
                    android:textSize="25sp"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="253dp" />

                <TextView
                    android:id="@+id/mailInProfile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="5dp"
                    android:fontFamily="@font/jockey_one"
                    android:text="qwerty@xmail.com"
                    android:textSize="15sp"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="253dp" />


                <Button
                    android:id="@+id/editProfileButton"
                    style="@style/Edit_Profile"
                    android:layout_width="170dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="5dp"
                    android:drawableRight="@drawable/img_1"
                    android:fontFamily="@font/jockey_one"
                    android:onClick="editProfileButtonIsClicked"
                    android:text="EDIT PROFILE"
                    android:textSize="12sp"
                    app:strokeColor="#76DE99"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="253dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"
                android:background="#858BCFFF"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/jockey_one"
                    android:paddingLeft="10dp"
                    android:text="BIO  :"
                    android:textSize="25dp"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="253dp" />

                <TextView
                    android:id="@+id/biography"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="10dp"
                    android:text="bio"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="253dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#E6F3FF"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView100"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:fontFamily="@font/jockey_one"
                    android:paddingLeft="10dp"
                    android:text="AGE    :"
                    android:textSize="25dp"
                    tools:layout_editor_absoluteX="21dp"
                    tools:layout_editor_absoluteY="288dp" />

                <TextView
                    android:id="@+id/ageInProfile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="10dp"
                    android:text="age"
                    tools:layout_editor_absoluteX="21dp"
                    tools:layout_editor_absoluteY="288dp" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center_vertical"
                android:background="#858BCFFF"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView111"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/jockey_one"
                    android:paddingLeft="10dp"
                    android:text="DEPARTMENT    :"
                    android:textSize="25dp"
                    tools:layout_editor_absoluteX="21dp"
                    tools:layout_editor_absoluteY="288dp" />

                <TextView
                    android:id="@+id/departmentInProfile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="department"
                    tools:layout_editor_absoluteX="21dp"
                    tools:layout_editor_absoluteY="288dp" />

            </LinearLayout>

            <Button
                android:id="@+id/logOutButton"
                style="@style/Log_Out"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30dp"
                android:drawableRight="@drawable/img_2"
                android:fontFamily="@font/jockey_one"
                android:text="LOG OUT"
                android:textSize="25sp"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="253dp" />

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>