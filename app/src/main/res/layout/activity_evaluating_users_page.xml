<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EvaluatingUsersPage">

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="25dp"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:background="#B3DEFF" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="25dp"
            android:layout_height="68dp"
            android:layout_weight="1"
            android:background="#8BCFFF" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="25dp"
            android:layout_height="55dp"
            android:layout_weight="1"
            android:background="#62B0FF" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="25dp"
            android:layout_height="43dp"
            android:layout_weight="1"
            android:background="#46B0FF" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="25dp"
            android:layout_height="55dp"
            android:layout_weight="1"
            android:background="#33BEFF" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="25dp"
            android:layout_height="62dp"
            android:layout_weight="1"
            android:background="#438AF8" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="25dp"
            android:layout_height="37dp"
            android:layout_weight="1"
            android:background="#6486FF" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_weight="1"
            android:background="#4094CD" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="25dp"
            android:layout_height="55dp"
            android:layout_weight="1"
            android:background="#4979B0" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="25dp"
            android:layout_height="65dp"
            android:layout_weight="1"
            android:background="#6993BD" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="25dp"
            android:layout_height="72dp"
            android:layout_weight="1"
            android:background="#739DE8" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="25dp"
            android:layout_height="34dp"
            android:layout_weight="1"
            android:background="#275583" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="#2058A8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/textView20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button7"
                style="@style/Evaluation_Page_Buttons_View"
                android:layout_width="140dp"
                android:layout_height="60dp"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_weight="1"
                android:fontFamily="@font/jockey_one"
                android:onClick="askMeLaterClicked"
                android:text="ASK ME LATER" />

            <Button
                android:id="@+id/button9"
                style="@style/Evaluation_Page_Buttons_View"
                android:layout_width="150dp"
                android:layout_height="60dp"
                android:layout_marginRight="30dp"
                android:layout_weight="1"
                android:fontFamily="@font/jockey_one"
                android:onClick="doNotAskAgainClicked"
                android:text="DO NOT ASK AGAIN" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button12"
                style="@style/Evaluation_Page_Buttons_View2"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/jockey_one"
                android:onClick="okClicked"
                android:text="OK" />
        </LinearLayout>
    </LinearLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/evRv"
        android:layout_width="391dp"
        android:layout_height="500dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7" />

</androidx.constraintlayout.widget.ConstraintLayout>